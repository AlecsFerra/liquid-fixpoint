fixpoint "--rewrite"
fixpoint "--allowho"

constant f : (func(0 , [int; int]))
define f (x : int) : int = {(x)}

constant g : (func(0 , [int; int]))
define g (a : int) : int = {(a)}


data Ty 0 = [
    | Cons {mkCons : func(0 , [int; int])}
]

constant Cons : (func(0 , [func(0 , [int; int]); Ty]))

expand [1 : True; 2 : True]

constraint:
  env []
  lhs {VV1 : Tuple | true }
  rhs {VV2 : Tuple | (Cons f = Cons g) }
  id 2 tag []