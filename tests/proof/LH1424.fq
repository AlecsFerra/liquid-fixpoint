fixpoint "--rewrite"

data Field 1 = [
       | FInt {}
     ]

define funky (ds : Field a,  kkk : a) : int = (((funky ds kkk) = (coerce (a ~ int) kkk)))

constant funky : (func(1 , [(Field @(0)); @(0); int]))

expand [1 : True; 
        2 : True] 

bind 1 tx : {VV : (Field bob) | []}
bind 2 ty : {VV : bob | []}

constraint:
  env [1; 2]
  lhs {VV : int | [VV = (funky tx ty)]}
  rhs {VV : int | [VV = (funky tx ty)]}
  id 1 tag []

