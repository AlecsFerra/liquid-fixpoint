
data LL 1 = [
  | emp { }
  | con { lHead : @(0), lTail : LL }
]

constant mkEmp : func(1,[LL @(0)])

bind 1 n : {n:int    | true}
bind 2 m : {m:int    | true}
bind 3 junk : {v:LL bool | true}
bind 4 punk : {v:LL int | con 1 emp = junk }

constraint:
  env [1; 2; 3; 4]
  lhs {v:int | (con n (con m mkEmp) = con m (con n emp)) }
  rhs {v:int | m = n }
  id 0 tag []
